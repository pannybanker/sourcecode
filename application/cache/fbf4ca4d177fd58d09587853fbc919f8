<script>
    (function ($) {
        $(document).ready(function () {
            $('ul.dropdown-menu [data-toggle=dropdown]').on('click', function (event) {
                event.preventDefault();
                event.stopPropagation();
                $(this).parent().siblings().removeClass('open');
                $(this).parent().toggleClass('open');
            });
        });
        $("#frmLogin").submit(function (event) {
            event.preventDefault();
            var username = $("#login_username").val();
            var password = $("#login_password").val();
            $('#loginbutton').html('<button type="button" desabled="desabled">Please wait... Sign-in request submitting....</button>');
            $.ajax({
                url: $("#frmLogin").attr('action'),
                type: 'POST',
                data: {username: username, password: password},
                success: function (result) {
                    if (result == 1) {
                        window.location.href = "http://pennybanker.com/users/dash";
                    } else if (result == 2) {
                        $("#login_fail").show();
                        $('#loginbutton').html('<button type="submit">Sign In</button>');
                        $("#login_fail").html('<strong>Danger!</strong> Account not activated. Please check register email in inbox OR spam');
                    } else if (result == 3) {
                        window.location.href = "http://pennybanker.com/frontend";
                    } else {
                        $("#login_fail").show();
                        $('#loginbutton').html('<button type="submit">Sign In</button>');
                        $("#login_fail").html('<strong>Danger!</strong> Login failed, please try again.');
                    }
                }
            });
        });


        $("#frmRegister").submit(function (event) {
            event.preventDefault();
            var name = $("#username").val();
            var email = $("#email").val();
            var password = $("#password").val();
            $('#registerbutton').html('<button type="button" desabled="desabled">Please wait... Register request submitting....</button>');

            $.ajax({
                url: $("#frmRegister").attr('action'),
                type: 'POST',
                data: {name: name, email: email, password: password},
                success: function (result) {
                    if (result == 0) {
                        $("#register_fail").show();
                        $('#registerbutton').html('<button type="submit">Join Now</button>');
                        $("#register_fail").html('<strong>Danger!</strong> Registration failed, please try again.');

                    } else if (result == 1) {
                        $("#register_fail").show();
                        $('#registerbutton').html('<button type="submit">Join Now</button>');
                        $("#register_fail").html('<strong>Danger!</strong> Registration failed, Email id already exist.');
                    } else {
                        $('#registerbutton').html('<button type="submit">Join Now</button>');
                        $("#frmRegister")[0].reset();
                        $("#register_success").show();

                    }
                }
            });
        });

        $("#frmRegister1").submit(function (event) {
            event.preventDefault();
            var name = $("#username1").val();
            var email = $("#email1").val();
            var password = $("#password1").val();
            $('#registerbutton1').html('<button type="button" desabled="desabled">Please wait... Register request submitting....</button>');

            $.ajax({
                url: $("#frmRegister1").attr('action'),
                type: 'POST',
                data: {name: name, email: email, password: password},
                success: function (result) {
                    if (result == 0) {
                        $("#register_fail1").show();
                        $('#registerbutton1').html('<button type="submit">Join Now</button>');
                        $("#register_fail1").html('<strong>Danger!</strong> Registration failed, please try again.');

                    } else if (result == 1) {
                        $("#register_fail1").show();
                        $('#registerbutton1').html('<button type="submit">Join Now</button>');
                        $("#register_fail1").html('<strong>Danger!</strong> Registration failed, Email id already exist.');
                    } else {
                        $('#registerbutton1').html('<button type="submit">Join Now</button>');
                        $("#frmRegister1")[0].reset();
                        $("#register_success1").show();

                    }
                }
            });
        });



        $("#frmForgot").submit(function (event) {
            event.preventDefault();

            var email = $("#frgemail").val();

            $('#btnFrg').html('Checking Email....').attr('disabled','disabled');

            $.ajax({
                url: 'http://pennybanker.com/mycon/forgotpassword',
                type: 'POST',
                data: {email: email},
                success: function (result) {
                    console.log(result);

                    if(result==0){
                        $("#forgot_fail").slideDown();
                        $("#forgot_success").slideUp();
                        $('#btnFrg').html('Reset Password....').removeAttr('disabled');
                    }else{
                        $("#forgot_success").slideDown();
                        $("#forgot_fail").slideUp();
                        $('#btnFrg').html('Reset Password....').removeAttr('disabled');
                    }

                }
            });
        });


        $("#frmremailcheck").submit(function (event) {
            event.preventDefault();

            var email = $("#txtemail").val();

           // $('#btnFrg').html('Checking Email....').attr('disabled','disabled');

            $.ajax({
                url: 'http://pennybanker.com/mycon/checkemail',
                type: 'POST',
                data: {email: email},
                success: function (result) {
                    console.log(result);

                    if(result==0){
                        $("#forgot_fail").slideDown();
                        //$("#forgot_success").slideUp();
                        //$('#btnFrg').html('Reset Password....').removeAttr('disabled');
                    }else{
                        $("#frmremailcheck").slideUp();
                        $("#frmreset").slideDown();

                        $("#forgot_fail").slideUp();
                        $("#hndPass").val(result);
                        //$('#btnFrg').html('Reset Password....').removeAttr('disabled');
                    }

                }
            });
        });


        $("#frmreset").submit(function (event) {
            event.preventDefault();

            var pwd = $("#txtPassword").val();
            var cpwd = $("#txtCPassword").val();
            var pkey = $("#hndPasskey").val();
            var id = $("#hndPass").val();


           // $('#btnFrg').html('Checking Email....').attr('disabled','disabled');

            if(pwd==cpwd) {
                $("#password_fail").slideUp();

                $.ajax({
                    url: 'http://pennybanker.com/mycon/setnewpassword',
                    type: 'POST',
                    data: {pwd: pwd, pkey: pkey, id: id},
                    success: function (result) {
                        console.log(result);
                        //$("#password_fail").slideDown();
                    $("#forgotp_success").slideDown();


                    }
                });
            }else{
                $("#password_fail").slideDown();

            }



        });






    })(jQuery);


    function get_new_details(news_id,source){


        $.ajax({
            type:"POST",
            url: 'http://pennybanker.com/' + "mycon/get_news_details",
            data:{news_id:news_id,source:source},
            dataType : "json",
            success: function(save_result) {
                //alert(save_result);
                $("#spnNImage").attr('src',save_result.image);
                $("#spnLCount").html(save_result.like);
                //$("#spnNText").html('<pre>'+save_result.text+'</pre>');
                $("#spnNText").html(save_result.text);
                $("#spnNTitle").html(save_result.title);
                $("#spnNSource").html(save_result.source);
                //$("#spnNImage").val(save_result.image);
                //$("#spnNImage").val(save_result.image);
                //$("#busi_country").val($("#zipcountry").val());
            }
        });

    }
</script>

</body>
</html>